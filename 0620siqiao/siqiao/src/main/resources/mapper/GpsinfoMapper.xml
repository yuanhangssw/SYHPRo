<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tianji.dam.mapper.GpsinfoMapper">
    
    <resultMap type="com.tianji.dam.domain.Gpsinfo" id="GpsinfoResult">
        <result property="id"    column="id"    />
        <result property="head"    column="head"    />
        <result property="messageid"    column="messageID"    />
        <result property="attribute"    column="attribute"    />
        <result property="idcard"    column="Idcard"    />
        <result property="waternumber"    column="waterNumber"    />
        <result property="warning"    column="warning"    />
        <result property="status"    column="status"    />
        <result property="latitude"    column="latitude"    />
        <result property="longitude"    column="longitude"    />
        <result property="gaocheng"    column="gaoCheng"    />
        <result property="speed"    column="speed"    />
        <result property="directionangle"    column="directionAngle"    />
        <result property="time"    column="time"    />
        <result property="electricity"    column="electricity"    />
        <result property="length"    column="length"    />
        <result property="check"    column="check"    />
        <result property="end"    column="end"    />
    </resultMap>

    <sql id="selectGpsinfoVo">
        select id, head, messageID, attribute, Idcard, waterNumber, warning, `status`, latitude, longitude, gaoCheng, speed, directionAngle, `time`, electricity, length, `check`, `end` from gpsinfo
    </sql>

    <select id="selectGpsinfoList" parameterType="com.tianji.dam.domain.Gpsinfo" resultMap="GpsinfoResult">
        <include refid="selectGpsinfoVo"/>
        <where>  
            <if test="head != null  and head != ''"> and head = #{head}</if>
            <if test="messageid != null  and messageid != ''"> and messageID = #{messageid}</if>
            <if test="attribute != null  and attribute != ''"> and attribute = #{attribute}</if>
            <if test="idcard != null  and idcard != ''"> and Idcard = #{idcard}</if>
            <if test="waternumber != null  and waternumber != ''"> and waterNumber = #{waternumber}</if>
            <if test="warning != null  and warning != ''"> and warning = #{warning}</if>
            <if test="status != null  and status != ''"> and `status` = #{status}</if>
            <if test="latitude != null  and latitude != ''"> and latitude = #{latitude}</if>
            <if test="longitude != null  and longitude != ''"> and longitude = #{longitude}</if>
            <if test="gaocheng != null  and gaocheng != ''"> and gaoCheng = #{gaocheng}</if>
            <if test="speed != null  and speed != ''"> and speed = #{speed}</if>
            <if test="directionangle != null  and directionangle != ''"> and directionAngle = #{directionangle}</if>
            <if test="time != null  and time != ''"> and `time` = #{time}</if>
            <if test="electricity != null  and electricity != ''"> and electricity = #{electricity}</if>
            <if test="length != null  and length != ''"> and length = #{length}</if>
            <if test="check != null  and check != ''"> and `check`= #{check}</if>
            <if test="end != null  and end != ''"> and `end` = #{end}</if>
        </where>
    </select>
    
    <select id="selectGpsinfoById" parameterType="Long" resultMap="GpsinfoResult">
        <include refid="selectGpsinfoVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertGpsinfo" parameterType="com.tianji.dam.domain.Gpsinfo" useGeneratedKeys="true" keyProperty="id">
        insert into gpsinfo
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="head != null">head,</if>
            <if test="messageid != null">messageID,</if>
            <if test="attribute != null">attribute,</if>
            <if test="idcard != null">Idcard,</if>
            <if test="waternumber != null">waterNumber,</if>
            <if test="warning != null">warning,</if>
            <if test="status != null">`status`,</if>
            <if test="latitude != null">latitude,</if>
            <if test="longitude != null">longitude,</if>
            <if test="gaocheng != null">gaoCheng,</if>
            <if test="speed != null">speed,</if>
            <if test="directionangle != null">directionAngle,</if>
            <if test="time != null">`time`,</if>
            <if test="electricity != null and electricity != ''">electricity,</if>
            <if test="length != null">length,</if>
            <if test="check != null">`check`,</if>
            <if test="end != null">`end`,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="head != null">#{head},</if>
            <if test="messageid != null">#{messageid},</if>
            <if test="attribute != null">#{attribute},</if>
            <if test="idcard != null">#{idcard},</if>
            <if test="waternumber != null">#{waternumber},</if>
            <if test="warning != null">#{warning},</if>
            <if test="status != null">#{status},</if>
            <if test="latitude != null">#{latitude},</if>
            <if test="longitude != null">#{longitude},</if>
            <if test="gaocheng != null">#{gaocheng},</if>
            <if test="speed != null">#{speed},</if>
            <if test="directionangle != null">#{directionangle},</if>
            <if test="time != null">#{time},</if>
            <if test="electricity != null and electricity != ''">#{electricity},</if>
            <if test="length != null">#{length},</if>
            <if test="check != null">#{check},</if>
            <if test="end != null">#{end},</if>
         </trim>
    </insert>

    <update id="updateGpsinfo" parameterType="com.tianji.dam.domain.Gpsinfo">
        update gpsinfo
        <trim prefix="SET" suffixOverrides=",">
            <if test="head != null">head = #{head},</if>
            <if test="messageid != null">messageID = #{messageid},</if>
            <if test="attribute != null">attribute = #{attribute},</if>
            <if test="idcard != null">Idcard = #{idcard},</if>
            <if test="waternumber != null">waterNumber = #{waternumber},</if>
            <if test="warning != null">warning = #{warning},</if>
            <if test="status != null">`status` = #{status},</if>
            <if test="latitude != null">latitude = #{latitude},</if>
            <if test="longitude != null">longitude = #{longitude},</if>
            <if test="gaocheng != null">gaoCheng = #{gaocheng},</if>
            <if test="speed != null">speed = #{speed},</if>
            <if test="directionangle != null">directionAngle = #{directionangle},</if>
          <!--  <if test="time != null">`time` = #{time},</if>-->
            <if test="electricity != null and electricity != ''">electricity = #{electricity},</if>
            <if test="length != null">length = #{length},</if>
            <if test="check != null">`check` = #{check},</if>
            <if test="end != null">`end` = #{end},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteGpsinfoById" parameterType="Long">
        delete from gpsinfo where id = #{id}
    </delete>

    <delete id="deleteGpsinfoByIds" parameterType="String">
        delete from gpsinfo where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>