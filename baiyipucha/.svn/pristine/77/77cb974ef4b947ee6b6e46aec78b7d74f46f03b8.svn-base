<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.BPatrolUnitPlaceMapper">
    
    <resultMap type="BPatrolUnitPlace" id="BPatrolUnitPlaceResult">
        <result property="id"    column="id"    />
        <result property="patrolUnit"    column="patrol_unit"    />
        <result property="patrolId"    column="patrol_id"    />
        <result property="detrimentType"    column="detriment_type"    />
        <result property="adress"    column="adress"    />
        <result property="lat"    column="lat"    />
        <result property="lon"    column="lon"    />
        <result property="inspector"    column="inspector"    />
        <result property="projectId"    column="project_id"    />
    </resultMap>

    <sql id="selectBPatrolUnitPlaceVo">
        select id, patrol_unit, patrol_id, detriment_type, adress, lat, lon, inspector, project_id from b_patrol_unit_place
    </sql>

    <select id="selectBPatrolUnitPlaceList" parameterType="BPatrolUnitPlace" resultMap="BPatrolUnitPlaceResult">
        <include refid="selectBPatrolUnitPlaceVo"/>
        <where>  
            <if test="patrolUnit != null "> and patrol_unit = #{patrolUnit}</if>
            <if test="patrolId != null  and patrolId != ''"> and patrol_id = #{patrolId}</if>
            <if test="detrimentType != null "> and detriment_type = #{detrimentType}</if>
            <if test="adress != null  and adress != ''"> and adress = #{adress}</if>
            <if test="lat != null  and lat != ''"> and lat = #{lat}</if>
            <if test="lon != null  and lon != ''"> and lon = #{lon}</if>
            <if test="inspector != null  and inspector != ''"> and inspector = #{inspector}</if>
            <if test="projectId != null "> and project_id = #{projectId}</if>
        </where>
    </select>
    
    <select id="selectBPatrolUnitPlaceById" parameterType="Long" resultMap="BPatrolUnitPlaceResult">
        <include refid="selectBPatrolUnitPlaceVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBPatrolUnitPlace" parameterType="BPatrolUnitPlace" useGeneratedKeys="true" keyProperty="id">
        insert into b_patrol_unit_place
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="patrolUnit != null">patrol_unit,</if>
            <if test="patrolId != null">patrol_id,</if>
            <if test="detrimentType != null">detriment_type,</if>
            <if test="adress != null">adress,</if>
            <if test="lat != null">lat,</if>
            <if test="lon != null">lon,</if>
            <if test="inspector != null">inspector,</if>
            <if test="projectId != null">project_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="patrolUnit != null">#{patrolUnit},</if>
            <if test="patrolId != null">#{patrolId},</if>
            <if test="detrimentType != null">#{detrimentType},</if>
            <if test="adress != null">#{adress},</if>
            <if test="lat != null">#{lat},</if>
            <if test="lon != null">#{lon},</if>
            <if test="inspector != null">#{inspector},</if>
            <if test="projectId != null">#{projectId},</if>
         </trim>
    </insert>

    <update id="updateBPatrolUnitPlace" parameterType="BPatrolUnitPlace">
        update b_patrol_unit_place
        <trim prefix="SET" suffixOverrides=",">
            <if test="patrolUnit != null">patrol_unit = #{patrolUnit},</if>
            <if test="patrolId != null">patrol_id = #{patrolId},</if>
            <if test="detrimentType != null">detriment_type = #{detrimentType},</if>
            <if test="adress != null">adress = #{adress},</if>
            <if test="lat != null">lat = #{lat},</if>
            <if test="lon != null">lon = #{lon},</if>
            <if test="inspector != null">inspector = #{inspector},</if>
            <if test="projectId != null">project_id = #{projectId},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBPatrolUnitPlaceById" parameterType="Long">
        delete from b_patrol_unit_place where id = #{id}
    </delete>

    <delete id="deleteBPatrolUnitPlaceByIds" parameterType="String">
        delete from b_patrol_unit_place where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>