    <sql id="selectBDykePreventionVo">
        select id, detriment_type, project_id, unit_id, detriment_level, leaks_number, through_number, drop_socket_number, nest_digging, charge_area, grouting_quantity, invest_capital, zoon_type, zoon_govern_type, create_time, detriment_length, one_detriment_length, two_detriment_length, three_detriment_length from b_dyke_prevention
