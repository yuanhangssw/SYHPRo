package com.ruoyi.system.domain;

import com.ruoyi.common.annotation.Excel;
import com.ruoyi.common.core.domain.BaseEntity;
import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

import java.util.List;
import java.util.stream.Collectors;

/**
 * 普查处管理对象 b_patrol_unit_place
 * 
 * @author ruoyi
 * @date 2024-04-08
 */
public class BPatrolUnitPlaceAll extends BaseEntity
{
    private static final long serialVersionUID = 1L;

    private BPatrolUnitPlace bPatrolUnitPlace;

    private List<BPatrol> patrols;

    public BPatrolUnitPlace getbPatrolUnitPlace() {
        return bPatrolUnitPlace;
    }

    public void setbPatrolUnitPlace(BPatrolUnitPlace bPatrolUnitPlace) {
        this.bPatrolUnitPlace = bPatrolUnitPlace;
    }

    public List<BPatrol> getPatrols() {
        return patrols;
    }

    public void setPatrols(List<BPatrol> patrols) {
        this.patrols = patrols;
    }

    @Override
    public String toString() {
            StringBuilder sb = new StringBuilder();
            sb.append("BPatrolUnitPlaceAll{");
            sb.append("bPatrolUnitPlace=").append(bPatrolUnitPlace != null ? bPatrolUnitPlace.toString() : "null").append(", ");
            sb.append("bPatrolList=").append(patrols.isEmpty()  ? patrols.stream()
                    .map(Object::toString)
                    .collect(Collectors.joining(", ")) : "null");
            sb.append("}");
            return sb.toString();
    }

}
